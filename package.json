  "scripts": {
    "postinstall": "node patch/crypto",
    "build:stats:prod": "ng build --stats-json --prod --configuration=mainnet-staging",
    "build:stats": "ng build --stats-json --aot",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "translate:extract": "gulp translate:extract",
    "translate:compile": "gulp translate:compile",
    "watch:dev": "ng serve --configuration=\"dev\"",
    "watch:dev:secure": "ng serve --ssl --configuration=\"dev\"",
    "widget": "gulp webpack",
    "build:dev": "ng build --configuration=\"dev\"",
    "build:mainnet": "ng build --configuration=\"mainnet\"",
    "build:mainnet-staging": "ng build --configuration=\"mainnet-staging\"",
    "build:testnet": "ng build --configuration=\"testnet\"",
    "build:e2e": "ng build --configuration=\"e2e\"",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod --configuration=\"dev\" && ng run helperbit:server:dev --bundleDependencies all",
    "lint": "eslint app/ --ext ts",
    "lintfix": "eslint app/ --fix  --ext ts",
    "start": "NODE_ENV='development' HB_ENV='dev' gulp serve",
    "e2e": "ng e2e",
    "test": "ng test",
    "test-hl": "ng test --watch false --progress false --browsers=ChromeHeadlessNoSandbox",
    "show-coverage": "firefox file://`pwd`/coverage/test/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "disabled_pre-push": "npm run lint"
    }
  },
